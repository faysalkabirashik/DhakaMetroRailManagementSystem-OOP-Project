<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="view.nayem.passenger.FeedbackSceneController">
    <children>
      <Label layoutX="222.0" layoutY="14.0" text="Report or Feedback">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="30.0" layoutY="58.0" text="Action Type:" />
      <Line endX="100.0" layoutX="300.0" layoutY="41.0" startX="-100.0" />
      <ComboBox fx:id="reportTypeComboBox" layoutX="30.0" layoutY="79.0" prefHeight="31.0" prefWidth="130.0" />
      <TextField fx:id="firstNameTextField" layoutX="30.0" layoutY="125.0" prefHeight="31.0" prefWidth="156.0" promptText="First Name" />
      <TextField fx:id="lastNameTextField" layoutX="218.0" layoutY="125.0" prefHeight="31.0" prefWidth="156.0" promptText="Last Name" />
      <TextField fx:id="phoneNumberTextField" layoutX="416.0" layoutY="125.0" prefHeight="31.0" prefWidth="156.0" promptText="Phone Number" />
      <TextArea fx:id="descriptionTextArea" layoutX="30.0" layoutY="188.0" prefHeight="153.0" prefWidth="550.0" promptText="Description:" />
      <Button layoutX="124.0" layoutY="355.0" mnemonicParsing="false" onAction="#backButtonOnClicked" text="Back" />
      <Button layoutX="391.0" layoutY="355.0" mnemonicParsing="false" onAction="#submitButtonOnClicked" text="Submit" />
   </children>
</AnchorPane>
