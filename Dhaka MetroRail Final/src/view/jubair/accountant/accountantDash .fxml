      <Button fx:id="transactionManagement" layoutX="430.0" layoutY="213.0" mnemonicParsing="false" onMouseClicked="#transactionManagementOnMouseClick" prefHeight="56.0" prefWidth="112.0" text="Transaction Management" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="backAccountant" layoutX="237.0" layoutY="311.0" mnemonicParsing="false" onMouseClicked="#backAccountantOnMouseClick" prefHeight="38.0" prefWidth="146.0" text="Back" wrapText="true" />
